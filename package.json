      "@babel/preset-typescript": "^7.10.4",
      "@types/jest": "^26.0.10",
      "babel-jest": "^26.3.0",
      "jest": "^26.4.1",
      "rxjs-marbles": "^6.0.1"
   },
   "jest": {
      "setupFilesAfterEnv": [
         "<rootDir>/src/plugins/jest-helper/global-setup.ts"
      ],
